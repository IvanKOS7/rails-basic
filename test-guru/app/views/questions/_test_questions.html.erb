<% resource.each do |test_question| %>
  <tr>
    <td> <%= test_question.body %></td>
    <td> <%= link_to 'Show', question_path(test_question) %></td>
    <td> <%= link_to 'Edit', edit_question_path(test_question) %></td>
    <td> <%= link_to 'Delete', question_path(test_question), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
